package com.hp.team7.vo;

import java.io.Serializable;

public class User implements Serializable {

	private static final long serialVersionUID = 6901840466440766530L;
	
	private int id;				//用户ID
	private String name;		//用户姓名
	private String password;	//用户密码
	private char is_locked;		//是否禁用
	private char force_chg_pwd;	//第一次登录是否修改密码
	private int language;		//用户语言
	
	private Role role;			//用户对应的角色
	private UserProfile userProfile;//用户详细信息
	
	
	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}
	/**
	 * @param id the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the password
	 */
	public String getPassword() {
		return password;
	}
	/**
	 * @param password the password to set
	 */
	public void setPassword(String password) {
		this.password = password;
	}
	/**
	 * @return the is_locked
	 */
	public char getIs_locked() {
		return is_locked;
	}
	/**
	 * @param is_locked the is_locked to set
	 */
	public void setIs_locked(char is_locked) {
		this.is_locked = is_locked;
	}
	/**
	 * @return the force_chg_pwd
	 */
	public char getForce_chg_pwd() {
		return force_chg_pwd;
	}
	/**
	 * @param force_chg_pwd the force_chg_pwd to set
	 */
	public void setForce_chg_pwd(char force_chg_pwd) {
		this.force_chg_pwd = force_chg_pwd;
	}
	/**
	 * @return the language
	 */
	public int getLanguage() {
		return language;
	}
	/**
	 * @param language the language to set
	 */
	public void setLanguage(int language) {
		this.language = language;
	}
	/**
	 * @return the role
	 */
	public Role getRole() {
		return role;
	}
	/**
	 * @param role the role to set
	 */
	public void setRole(Role role) {
		this.role = role;
	}
	/**
	 * @return the userProfile
	 */
	public UserProfile getUserProfile() {
		return userProfile;
	}
	/**
	 * @param userProfile the userProfile to set
	 */
	public void setUserProfile(UserProfile userProfile) {
		this.userProfile = userProfile;
	}


}
