package com.hp.team7.vo;

import java.io.Serializable;

public class DealerCat implements Serializable {

	private static final long serialVersionUID = 996738132352030278L;
	
	private int id;		//经销商种类ID
	private String name;//经销商种类名字
	private String description;//经销商种类描述
	
	
	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}
	/**
	 * @param id the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the description
	 */
	public String getDescription() {
		return description;
	}
	/**
	 * @param description the description to set
	 */
	public void setDescription(String description) {
		this.description = description;
	}
	

}
