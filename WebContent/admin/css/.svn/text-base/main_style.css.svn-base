@CHARSET "UTF-8";

body {
	font: normal 15px auto "Trebuchet MS", Verdana, Arial, Helvetica,
		sans-serif;
	color: #13a;
	/*  background: #EEEEEE url(../images/bg.gif) left top repeat-x; */
	text-align: center;
	margin: 0;
	padding: 0;
}


table{
	border-style: solid;
	border-width: 1px;
	

}
table.one1{
	
	border-width: 0px;
	
}


hr {
	width: 90%;
	height: 3px;
	border: none;
	border-top: 3px double skyblue;
}

input[type="button"] {
	border: 3px double #7BC76B;
	background-color: white;
	font-size: 12px;
	padding-top: 1px;
	cursor:pointer;
	FILTER: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,
		StartColorStr=#ffffff, EndColorStr=#B3D997

}


#footer {
	padding-bottom: 10px;
	text-align: center;
	margin-top: 50px;
	margin-bottom: 0px;
	width: 100%;
	height: 50px; /*脚部的高度 	background: #6cf;*/
	clear: both;
}

.links a:hover {
	color: #C11;
	text-decoration: underline;
}

.links a:link,.links a:visited {
	color: #333;
	text-decoration: none;
}
